{
  "version": "0.2.1",
  "defaults": {
  },
  "configurations": [
    {
      "name": "Debug",
      "type": "cppvsdbg",
      "project": "CMakeLists.txt",
      "exe": "${workspaceRoot}\\..\\..\\Godot_v4.5-stable_win64.exe",
      "args": [
        "--verbose",
        "--path",
        "${workspaceRoot}\\..\\godot"
      ],
      "cwd": "${workspaceRoot}\\..\\godot"
    },
    {
      "name": "LaunchConsole",
      "type": "launch",
      "exe": "${workspaceRoot}\\..\\..\\Godot_v4.5-stable_win64_console.exe",
      "project": "CMakeLists.txt",
      "args": [
        "--verbose",
        "--path",
        "${workspaceRoot}\\..\\godot"
      ],
      "cwd": "${workspaceRoot}\\..\\godot"
    },
    {
      "name": "Edit",
      "type": "cppvsdbg",
      "console": "external",
      "exe": "${workspaceRoot}\\..\\..\\Godot_v4.5-stable_win64.exe",
      "project": "CMakeLists.txt",
      "args": [
        "--verbose",
        "--editor",
        "--path",
        "${workspaceRoot}\\..\\godot"
      ],
      "cwd": "${workspaceRoot}\\..\\godot"
    },
    {
      "name": "DISPLAY VARIABLE",
      "type": "launch",
      "project": "CMakeLists.txt",
      "console": "internalConsole",
      "exe": "C:\\Windows\\System32\\cmd.exe",
      "args": [
        "/C",
        "echo projectDir is: ${workspaceRoot} && pause"
      ]
    }
  ]
}
